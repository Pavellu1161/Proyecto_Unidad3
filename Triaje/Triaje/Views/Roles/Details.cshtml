@model Triaje.Models.Rol

@{
    ViewData["Title"] = "Details";
}

<h4 class="bg-dark" style="color:orange; font-family:Vogue; text-align:center; text-decoration:underline">Detalles Rol</h4>

<div>
    <hr />
    <dl class="row" style="font-family:Garamond">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NombreRol)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombreRol)
        </dd>
    </dl>
</div>
<div>
    <a class="btn bg-dark" style="color:orange; font-family:Vogue;" asp-action="Edit" asp-route-id="@Model.IdRol">Editar</a> 
    <a class="btn bg-dark" style="color:orange; font-family:Vogue;" asp-action="Index">Regresar</a>
</div>
