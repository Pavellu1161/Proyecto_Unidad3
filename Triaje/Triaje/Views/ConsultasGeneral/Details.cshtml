@model Triaje.Models.Consulta

@{
    ViewData["Title"] = "Details";
}
<h4 class="text-info" style="font-family:Vogue; text-decoration:underline; text-align:center">Detalles Consulta</h4>
<hr />
<div>
    <dl class="row" style="font-family:Garamond;">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Fecha_Hora)
        </dt>
        <dd class="col-sm-2">
            @Html.DisplayFor(model => model.Fecha_Hora)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.HoraTriaje)
        </dt>
        <dd class="col-sm-2">
            @Html.DisplayFor(model => model.HoraTriaje)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Signo)
        </dt>
        <dd class="col-sm-2">
            @Html.DisplayFor(model => model.Signo.Nombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Paciente)
        </dt>
        <dd class="col-sm-2">
            @Html.DisplayFor(model => model.Paciente.IdentidadPaciente)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Personal)
        </dt>
        <dd class="col-sm-5">
            @Html.DisplayFor(model => model.Personal.IdentidadPersonal)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.MotivoConsulta)
        </dt>
        <dd class="col-sm-10">
            <textarea disabled class="col-12" rows="6">@Html.DisplayFor(model => model.MotivoConsulta)</textarea>
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-info" style="font-family:Vogue" asp-action="Edit" asp-route-id="@Model.IdConsulta">Editar</a> 
    <a class="btn btn-info" style="font-family:Vogue" asp-action="Index">Regresar</a>
</div>
